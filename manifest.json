{
	"manifest_version": 2,
	"name": "Socialex",
	"version": "0.0.1",
	"author": "Jakub Rychl√Ω",
	"description": "Super webextension for our best app for FakeHacks.",
	"content_scripts": [
		{
			"matches": [
				"*://*.facebook.com/*"
			],
			"js": [
				"dist/socialbot.js"
			]
		}
	],
	"icons": {
		"48": "icons/48.png"
	},
	"background": {
		"scripts": ["src/background.js"]
	},
	"browser_action": {
		"default_popup": "static/options.html",
		"default_icon": {
			"48": "icons/48.png"
		}
	},
	"permissions": [
		"storage",
		"<all_urls>"
	]
}
